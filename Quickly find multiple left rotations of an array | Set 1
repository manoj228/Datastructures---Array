//Space optimized approach
#include <stdio.h>

void leftrotate(int a[],int n,int k)
{
    for(int i=k;i<k+n;i++)
    {
        printf("%d\t",a[i%n]);
    }
    printf("\n");
}

int main()
{
   int a[]={1,3,5,7,9};
   int n=sizeof(a)/sizeof(a[0]);
   int k;
   
   k=1;
   leftrotate(a,n,k);
   
   k=2;
   leftrotate(a,n,k);
   
    return 0;
}
